{
  "name": "ng-ag-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "frontend:start": "ng serve",
    "backend:dev": "ts-node-dev --respawn --transpile-only projects/chat-backend/src/server.ts",
    "backend:build": "tsc --project projects/chat-backend/tsconfig.json",
    "backend:start": "node dist/chat-backend/server.js",
    "frontend:build": "ng build",
    "frontend:watch": "ng build --watch --configuration development",
    "frontend:test": "ng test",
    "all:dev": "concurrently --kill-others \"npm run frontend:start\" \"npm run backend:build && npm run backend:start\"",
    "all:build": "npm run frontend:build && npm run backend:build"
  },
  "private": true,
  "dependencies": {
    "@ag-ui/client": "^0.0.41",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@tailwindcss/postcss": "^4.1.17",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "openai": "^6.9.1",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.6",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.1",
    "@types/socket.io": "^3.0.1",
    "concurrently": "^9.2.1",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.9.3"
  }
}
